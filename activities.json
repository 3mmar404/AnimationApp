      "title": "Step Aerobics",
      "icon": "ðŸ‘Ÿ",
      "description": "High-energy cardio using a raised platform. 32-Count System.",
      "bpm_range": "128 - 132",
      "sections": [
        {
          "type": "moves",
          "title": "Core Moves & Vocabulary",
          "items": [
            {
              "name": "Basic Step",
              "description": "Up Right, Up Left, Down Right, Down Left.",
              "cue": "Up, Up, Down, Down / Sali, Sali, Scendi, Scendi"
            },
            {
              "name": "V-Step",
              "description": "Step Wide Right, Step Wide Left, In Right, In Left (Arms in 'V' shape).",
              "cue": "Open, Open, Close, Close / Apri, Apri"
            },
            {
              "name": "Knee Lift (Repeater)",
              "description": "Step up, lift knee, step down. Can be single or 3x repeater.",
              "cue": "Knee Up! / Ginocchio Su!"
            },
            {
              "name": "L-Step",
              "description": "Step up corner, knee lift side, exit side (L Shape).",
              "cue": "L-Step / Passo a L"
            },
            {
              "name": "Turn Step",
              "description": "Step up side, turn body, step down.",
              "cue": "Turn / Gira"
            },
            {
              "name": "Over the Top",
              "description": "Walk laterally over the bench.",
              "cue": "Over the top / Dall'altra parte"
            },
            {
              "name": "Mambo",
              "description": "Rock forward on the floor/bench, recover, step back.",
              "cue": "Mambo!"
            }
          ]
        },
        {
          "type": "routines",
          "title": "Standard Combos",
          "items": [
            {
              "title": "Beginner Combo (32 Counts)",
              "steps": [
                "(1-8) 2 x Basic Steps (Right Lead)",
                "(9-16) 2 x V-Steps (Add Arms)",
                "(17-24) 3 x Knee Repeaters (Changes lead leg to Left)",
                "(25-32) March on the floor"
              ]
            }
          ]
        },
